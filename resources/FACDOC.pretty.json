            }
            ,"DocumentLink": {
                "type": "string"
               ,"title": "Dcoument Links"
               ,"description": "URLS for documents that EPA has colleted that are related to this Case."
            }
         }
         ,"required": [
             "CaseNumber"
            ,"FacilityName"
            ,"PublishDate"
            ,"EPAProgram"
            ,"DocumentLink"
         ]
      }
